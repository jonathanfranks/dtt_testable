# Ignore directories generated by Composer
/drush/contrib/
/vendor/
/web/core/
/web/modules/contrib/
/web/themes/contrib/
/web/profiles/contrib/
/web/libraries/

# Ignore sensitive information
/web/sites/*/settings.php
/web/sites/*/settings.local.php

# Ignore Drupal's file directory
/web/sites/*/files/

# Ignore SimpleTest multi-site environment.
/web/sites/simpletest

# Ignore files generated by PhpStorm
/.idea/

# Ignore .env files as they are personal
/.env

# Ignore user's behat.yml
/tests/behat.yml
/tests/failures/*
!/tests/failures/.gitkeep

# Ignore local computer stuff.
.DS_Store

.editorconfig
.gitattributes

# Ignore database backups
*.sql
*.sql.gz
*.zip

# Ignore the keys diretory. This is used for secret files and differs by environment.
/keys

# Ignore Office temp files
*.docx#

# Ignore the saml symlink.
web/simplesaml
